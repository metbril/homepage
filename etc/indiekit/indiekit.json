{
  "categories": [],
  "syndicate-to": [{
    "uid": "https://twitter.com/robertvanbregt/",
    "name": "@robertvanbregt on Twitter"
  }, {
    "uid": "https://news.indieweb.org/nl",
    "name": "IndieNews NL"
  }],
  "media-endpoint" : "https://indiekit-robertvanbregt.herokuapp.com/media",
  "post-types": {
    "note": {
      "template": "etc/indiekit/template/note.njk",
      "post": {
        "path": "content/notitie/{{ published | date('yyyy-MM-dd-X') }}.md",
        "url": "{{ published | date('yyyy/MM/dd/X/') }}"
      }
    },
    "like": {
      "post": {
        "path": "content/like/{{ published | date('yyyy-MM-dd-X') }}.md",
        "url": "{{ published | date('yyyy/MM/dd/X/') }}"
      }
    },
    "reply": {
      "post": {
        "path": "content/reactie/{{ published | date('yyyy-MM-dd-X') }}.md",
        "url": "{{ published | date('yyyy/MM/dd/X/') }}"
      }
    },
    "rsvp": {
      "template": "etc/indiekit/template/slug.njk",
      "post": {
        "path": "content/rsvps/{{ published | date('yyyy-MM-dd-X') }}.md",
        "url": "{{ published | date('yyyy/MM/dd/X/') }}"
      }
    },
    "bookmark": {
      "post": {
        "path": "content/bladwijzer/{{ published | date('yyyy-MM-dd') }}-{{ slug }}.md",
        "url": "{{ published | date('yyyy/MM/dd') }}/{{ slug }}/"
      }
    },
    "article": {
      "post": {
        "path": "content/artikel/{{ published | date('yyyy/MM/dd') }}/{{ slug }}/index.md",
        "url": "{{ published | date('yyyy/MM/dd') }}/{{ slug }}/"
      }
    }
  }
}
